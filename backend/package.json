{
  "name": "product-feed-api",
  "version": "1.0.0",
  "description": "API for fetching and storing yarn product feeds from partner-ads.com",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "tsx src/db/migrate.ts",
    "feed:import": "tsx src/feed/import.ts"
  },
  "keywords": [
    "yarn",
    "knitting",
    "affiliate",
    "product-feed"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.21.1",
    "pg": "^8.13.1",
    "fast-xml-parser": "^4.5.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
